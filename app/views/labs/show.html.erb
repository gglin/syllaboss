<%= render partial: "layouts/header_materials_show", locals: {material: @lab} %>

<div class="matter">
  <div class="container-fluid">

    <%= render "layouts/messages" %>
    
    <div class="row-fluid">

      <div class="span9 maxspan">
        <div class="widget wlightblue mainwidget">
          <div class="widget-head">
            <div class="pull-left">Lab</div>
            <div class="widget-icons pull-right">
              <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a> 
              <!-- <a href="#" class="wclose"><i class="icon-remove"></i></a> -->
            </div>
            <div class="clearfix"></div>
          </div>

          <div class="widget-content">
            <div class="padd lowtop">
              <p>
                <h2><%= @lab.title %></h2>
              </p>
              <p>
                <%= @lab.lab_url %>
              </p>
            </div>
          </div>
        </div>
      </div>

    </div>
    
    <%= render partial: "layouts/footer_materials_show", locals: {material: @lab} %>

  </div>
</div>