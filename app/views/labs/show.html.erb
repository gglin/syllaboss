<div class="matter">
  <div class="container-fluid">
    <div class="row-fluid">
      <p id="notice"><%= notice %></p>

      <p>
        <h3><b>Name:</b>
        <%= @lab.name %></h3>
      </p>
      <p>
        <b>Gist:</b>
        <%=link_to @lab.lab_url, @lab.http_link(@lab.lab_url), :target => "_blank" %>
      </p>

      <%= link_to 'Edit', edit_lab_path(@lab) %> |
      <%= link_to 'Back', labs_path %>
    </div>
  </div>
</div>